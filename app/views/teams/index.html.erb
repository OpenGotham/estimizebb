<h1>Listing teams</h1>

<table>
  <tr>
    <th>Name</th>
    <th>League</th>
    <th>Division</th>
    <th>Manager</th>
    <th>Wins</th>
    <th>Losses</th>
    <th>Founded</th>
    <th>Mascot</th>
    <th>Ballpark</th>
    <th>Logo url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%= team.name %></td>
    <td><%= team.league %></td>
    <td><%= team.division %></td>
    <td><%= team.manager %></td>
    <td><%= team.wins %></td>
    <td><%= team.losses %></td>
    <td><%= team.founded %></td>
    <td><%= team.mascot %></td>
    <td><%= team.ballpark %></td>
    <td><%= team.logo_url %></td>
    <td><%= link_to 'Show', team %></td>
    <td><%= link_to 'Edit', edit_team_path(team) %></td>
    <td><%= link_to 'Destroy', team, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Team', new_team_path %>
